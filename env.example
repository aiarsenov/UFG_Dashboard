NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
# Service Role Key для Admin API (только для серверных маршрутов)
# Найти в Supabase: Settings → API → service_role key
SUPABASE_SERVICE_ROLE_KEY=
# Email админа (можно указать несколько через запятую)
# По умолчанию: admin@example.com
ADMIN_EMAIL=

# DataLens приватный ключ для генерации токена встраивания
# Полный приватный ключ RSA (включая -----BEGIN RSA PRIVATE KEY----- и -----END RSA PRIVATE KEY-----)
DATALENS_PRIVATE_KEY=
# ID дашборда для встраивания (опционально, если не указан, используется s49hscam1mbed)
DATALENS_EMBED_ID=s49hscam1mbed

# Используется библиотекой Next.js
NEXT_TELEMETRY_DISABLED=1


